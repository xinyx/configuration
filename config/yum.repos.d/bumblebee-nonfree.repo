[bumblebee-nonfree]
name=bumblebee nonfree for fedora Linux $releasever - $basearch - Base
baseurl=http://install.linux.ncsu.edu/pub/yum/itecs/public/bumblebee-nonfree/fedora$releasever/
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-bumblebee-nonfree-public

[bumblebee-nonfree-source]
name=bumblebee nonfree for fedora Linux $releasever - $basearch - Source
baseurl=http://install.linux.ncsu.edu/pub/yum/itecs/public/bumblebee-nonfree/fedora$releasever/SRPMS
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-bumblebee-nonfree-public
gpgcheck=1
